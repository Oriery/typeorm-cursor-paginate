{
  "name": "typeorm-paginator",
  "version": "0.6.2",
  "description": "TypeORM query builder pagination library.",
  "keywords": [
    "pagination",
    "paginator",
    "typeorm",
    "typescript"
  ],
  "author": "Changwan Jun <wan2land@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://github.com/wan2land/typeorm-paginator.git"
  },
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "prepack": "npm run test && npm run build",
    "build": "rimraf ./lib && swc src -d lib --config-file ./build.swcrc && tsc",
    "test": "npm run build && npm run test:lint && npm run test:unit",
    "test:lint": "eslint src/**/*.ts",
    "test:unit": "jest src --runInBand"
  },
  "dependencies": {
    "regenerator-runtime": "^0.14.1"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.0",
    "@eslint/js": "^9.22.0",
    "@swc/cli": "0.6.0",
    "@swc/core": "1.11.9",
    "@swc/jest": "0.2.37",
    "@types/jest": "29.5.14",
    "@types/node": "22.13.10",
    "browserslist": "4.24.4",
    "eslint": "9.22.0",
    "eslint-config-prettier": "^10.1.1",
    "eslint-plugin-prettier": "^5.2.3",
    "globals": "^16.0.0",
    "jest": "29.7.0",
    "prettier": "^3.5.3",
    "rimraf": "6.0.1",
    "sqlite3": "5.1.7",
    "typeorm": "0.3.21",
    "typescript": "5.8.2",
    "typescript-eslint": "^8.26.1"
  }
}
